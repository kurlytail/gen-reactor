{
    "name": "@kurlytail/gen-reactor",
    "description": "REACTOR project generator",
    "version": "0.0.1",
    "main": "dist/reactor.min.js",
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.4.0",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
        "@babel/preset-env": "^7.4.2",
        "@babel/preset-flow": "^7.0.0",
        "@babel/register": "^7.4.0",
        "@kurlytail/gen-log": "^1.2.1",
        "@kurlytail/gen-pom": "^1.2.23",
        "@kurlytail/gen-sgen": "^1.2.9",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.5.0",
        "babel-loader": "^8.0.5",
        "babel-plugin-module-resolver": "^3.2.0",
        "child_process": "^1.0.2",
        "circular-dependency-plugin": "^4.2.1",
        "eslint": "^4.19.1",
        "eslint-config-prettier": "^2.6.0",
        "eslint-import-resolver-babel-module": "^4.0.0",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-babel": "^4.1.2",
        "eslint-plugin-flowtype": "^3.4.2",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jest": "^22.1.2",
        "eslint-plugin-prettier": "^2.3.1",
        "flow-bin": "^0.94.0",
        "jest": "^23.6.0",
        "jest-junit-reporter": "^1.1.0",
        "jsdoc": "^3.5.5",
        "prettier": "^1.16.0",
        "source-map-loader": "^0.2.3",
        "webpack": "^3.5.5",
        "webpack-node-externals": "^1.7.2"
    },
    "license": "UNLICENSED",
    "jest": {
        "testResultsProcessor": "./node_modules/jest-junit-reporter",
        "moduleDirectories": [
            "node_modules",
            "./src/js"
        ],
        "collectCoverage": true,
        "coverageReporters": [
            "text",
            "html"
        ],
        "collectCoverageFrom": [
            "src/js/**/*.js",
            "!**/node_modules/**"
        ],
        "testPathIgnorePatterns": [
            "testoutput",
            ".generated"
        ]
    },
    "scripts": {
        "docs": "jsdoc -c jsdoc.json && npx opn-cli ./docs/index.html",
        "gen": "sgen -g sgen -g log -d design.json",
        "build": "NODE_ENV=\"debug\" webpack --display-modules --display-reasons",
        "lint": "eslint src",
        "test": "jest tests",
        "test:watch": "jest --watch tests",
        "test:ci": "jest --ci tests",
        "clean": "rimraf ./docs ./dist ./coverage test-report.xml"
    },
    "dependencies": {
        "@kurlytail/gen-lib": "^1.2.9"
    }
}
